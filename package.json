{
  "name": "@alvarosabu/tresjs",
  "version": "0.0.0-development",
  "description": "A composable way of adding 3D to your Vue application",
  "author": "Alvaro Saburido <hola@alvarosaburido.dev>",
  "types": "dist/tresjs.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alvarosabu/tresjs.git"
  },
  "contributors": [
    "Alvaro <hola@alvarosaburido.dev>"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/alvarosabu/tresjs/issues"
  },
  "homepage": "https://github.com/alvarosabu/tresjs#readme",
  "directories": {
    "doc": "docs",
    "playground": "playground"
  },
  "main": "dist/tres.cjs.js",
  "module": "dist/tres.es.js",
  "browser": "dist/tres.es.js",
  "unpkg": "dist/tres.iife.js",
  "jsdelivr": "dist/tres.iife.js",
  "exports": {
    ".": {
      "import": "./dist/tres.es.js",
      "require": "./dist/tres.umd.js"
    },
    "./dist/themes/": "./dist/themes/"
  },
  "scripts": {
    "dev": "vite",
    "build:playground": "vite build",
    "build": "vue-tsc --noEmit && vite build --config vite-lib.config.ts",
    "preview": "vite preview",
    "semantic-release": "semantic-release",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "vue",
    "3d",
    "threejs",
    "three",
    "threejs-vue"
  ],
  "dependencies": {
    "three": "^0.136.0",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@alvarosabu/prettier-config": "^1.1.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/three": "^0.136.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vueuse/core": "^7.5.4",
    "canvas": "^2.9.0",
    "gl": "^5.0.0",
    "jsdom": "^19.0.0",
    "pathe": "^0.2.0",
    "rollup-plugin-copy": "^3.4.0",
    "semantic-release": "^19.0.2",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-windicss": "^1.6.3",
    "vitest": "^0.2.3",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.29.8",
    "windicss": "^3.4.3"
  }
}
